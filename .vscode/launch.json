  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "pnpm install",
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "env": {
        "PORT": "7702",
        "PROXY": "http://localhost:7322"
      }
    }
  ]
}
